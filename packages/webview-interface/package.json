{
    "name": "@abbieben/webview-interface",
    "version": "2.0.14",
    "description": "This is the description for the plugin seed. This is what will be published to NPM.",
    "main": "./webview-interface",
    "sideEffects": false,
    "typings": "./webview-interface.d.ts",
    "nativescript": {
        "platforms": {
            "android": "6.0.0",
            "ios": "6.0.0"
        }
    },
    "scripts": {
        "build": "npm run tsc && npm run readme",
        "build.all": "npm run build && npm run build.angular",
        "build.angular": "../../node_modules/.bin/ng-packagr -p ../../src/webview-interface/angular/ng-package.json -c ../../src/webview-interface/angular/tsconfig.json",
        "readme": "../../node_modules/.bin/readme generate -c ../../tools/readme/blueprint.json",
        "tsc": "cpy '**/*.{d.ts,aar,jar,java,xml}' '../../packages/webview-interface' --parents --cwd=../../src/webview-interface && tsc -skipLibCheck -d",
        "clean": "../../node_modules/.bin/rimraf ./*.d.ts ./*.js ./*.js.map"
    },
    "keywords": [
        "NativeScript",
        "JavaScript",
        "Android",
        "iOS",
        "Angular",
        "React",
        "Svelte",
        "Vue"
    ],
    "bugs": {
        "url": "https://github.com/abbieben/webview-interface/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/abbieben/webview-interface"
    },
    "publishConfig": {
        "access": "public"
    },
    "license": "Apache-2.0",
    "readmeFilename": "README.md",
    "gitHead": "6df3ca444772bee4385c36ee821b86c84dc39527"
}
